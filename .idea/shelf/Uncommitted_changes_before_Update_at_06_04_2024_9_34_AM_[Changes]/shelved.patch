+<script>
+    // تعداد دایره‌ها
+    var numberOfCircles = 10;
+
+    // ساخت یک شبیه‌سازی
+    var simulation = d3.forceSimulation()
+        .force("center", d3.forceCenter(250, 150)) // مرکز دایره‌ها
+        .force("charge", d3.forceManyBody().strength(0.5)) // نیروی جاذبه بین دایره‌ها
+
+    // داده‌های مربوط به دایره‌ها
+    var nodes = d3.range(numberOfCircles).map(function() {
+        return { radius: 20 }; // اندازه دایره‌ها
+    });
+
+    // ایجاد دایره‌ها
+    var circles = d3.select("svg").selectAll("circle")
+        .data(nodes)
+        .enter().append("circle")
+        .attr("r", function(d) { return d.radius; })
+        .attr("fill", "steelblue")
+        .call(d3.drag() // افزودن قابلیت drag برای دایره‌ها
+            .on("start", dragstarted)
+            .on("drag", dragged)
+            .on("end", dragended))
+        .on("click", function(d, i) { // رویداد کلیک بر روی دایره
+            // ارسال اطلاعات دایره به صفحه دیگر
+            var dataToSend = { index: i, radius: d.radius };
+            fetch("post.php", {
+                method: "POST",
+                body: JSON.stringify(dataToSend),
+                headers: {
+                    "Content-Type": "application/json"
+                }
+            })
+                .then(response => {
+                    // پردازش پاسخ
+                })
+                .catch(error => {
+                    // پردازش خطا
+                });
+        });
+
+    // قرار دادن دایره‌ها در محیط
+    circles.attr("cx", function(d, i) { return (i + 1) * 50; })
+        .attr("cy", 150); // محور Y ثابت است
+
+    // تابع‌های drag
+    function dragstarted(event, d) {
+        if (!event.active) simulation.alphaTarget(.03).restart();
+        d.fx = d.x;
+        d.fy = d.y;
+    }
+
+    function dragged(event, d) {
+        d.fx = event.x;
+        d.fy = event.y;
+    }
+
+    function dragended(event, d) {
+        if (!event.active) simulation.alphaTarget(.03);
+        d.fx = null;
+        d.fy = null;
+    }
+</script>
